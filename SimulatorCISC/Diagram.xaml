        <Button x:Name="btnViewDecimal" Content="DECIMAL" HorizontalAlignment="Left" Margin="383,10,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-0.067,0.6" BorderBrush="{x:Null}" Foreground="White" Background="#FF8C57FF" Click="BtnViewDecimal_Click"/>
        <Button x:Name="btnINTR" Content="INTR" HorizontalAlignment="Left" Margin="480,10,0,0" VerticalAlignment="Top" Width="75" Background="#FF8C57FF" Foreground="White" BorderBrush="{x:Null}" Click="BtnINTR_Click"/>

    </Grid>
</Window>
